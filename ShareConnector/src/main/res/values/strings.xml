      <string name="js_qbittorrent_login_script">
          var usernameField = document.querySelector(\'input[name="username"], input[id="username"], input[type="text"], #username\');
          var passwordField = document.querySelector(\'input[name="password"], input[id="password"], input[type="password"], #password\');
          var loginButton = document.querySelector(\'input[type="submit"], button[type="submit"], #login, .login-button, input[value*="Login"], button[onclick*="login"]\');

           if (usernameField &amp;&amp; passwordField &amp;&amp; loginButton) {
              usernameField.value = \'%1$s\';
              passwordField.value = \'%2$s\';
              loginButton.click();
           }
      </string>
      <string name="js_utorrent_login_script">
          var passwordField = document.querySelector(\'input[name="password"], input[type="password"]\');
          var loginButton = document.querySelector(\'input[type="submit"], button[type="submit"]\');

           if (passwordField &amp;&amp; loginButton) {
              passwordField.value = \'%s\';
              loginButton.click();
          }
      </string>
      <string name="js_auth_verification_script">
          var isOnLoginPage = document.querySelector(\'input[name="username"], input[name="password"], .login-form\') !== null;
          isOnLoginPage ? "false" : "true";
      </string>
      <string name="js_qbittorrent_add_torrent_script">
          var url = \'%s\';

          // Try multiple selectors for URL input field
          var urlSelectors = [
              \'input[name="urls"]\',
              \'textarea[name="urls"]\',
              \'input[placeholder*="URL"]\',
              \'input[placeholder*="url"]\',
              \'input[placeholder*="magnet"]\',
              \'textarea[placeholder*="URL"]\',
              \'textarea[placeholder*="url"]\',
              \'textarea[placeholder*="magnet"]\',
              \'input[id*="url"]\',
              \'textarea[id*="url"]\',
              \'input[type="url"]\',
              \'.form-control[placeholder*="URL"]\'
          ];

          var urlField = null;
          for (var i = 0; i &lt; urlSelectors.length; i++) {
              urlField = document.querySelector(urlSelectors[i]);
              if (urlField) break;
          }

          if (urlField) {
              urlField.value = url;
              urlField.dispatchEvent(new Event(\'input\', { bubbles: true }));
              urlField.dispatchEvent(new Event(\'change\', { bubbles: true }));

              // Try multiple selectors for submit buttons
              var submitSelectors = [
                  \'input[type="submit"]\',
                  \'button[type="submit"]\',
                  \'input[value*="Download"]\',
                  \'button[onclick*="add"]\',
                  \'button[onclick*="download"]\',
                  \'input[value*="Add"]\',
                  \'button:contains("Add")\',
                  \'button:contains("OK")\',
                  \'button:contains("Download")\'
              ];

              var submitButton = null;
              for (var j = 0; j &lt; submitSelectors.length; j++) {
                  submitButton = document.querySelector(submitSelectors[j]);
                  if (submitButton) break;
              }

              if (submitButton) {
                  submitButton.click();
                  "success";
              } else {
                  // Try to find modern qBittorrent UI "Add Torrent" button
                  var contextMenu = document.querySelector(\'.context-menu, .dropdown-menu\');
                  if (!contextMenu) {
                      // Try to trigger context menu or find toolbar buttons
                      var toolbarButtons = [
                          \'button[title*="Add"]\',
                          \'a[title*="Add"]\',
                          \'.toolbar button[title*="Add"]\',
                          \'.toolbar a[title*="Add"]\',
                          \'button[onclick*="add"]\',
                          \'a[onclick*="add"]\',
                          \'.material-icons:contains("add")\',
                          \'button:contains("Add")\',
                          \'a:contains("Add")\'
                      ];

                      for (var k = 0; k &lt; toolbarButtons.length; k++) {
                          var addButton = document.querySelector(toolbarButtons[k]);
                          if (addButton) {
                              addButton.click();
                              break;
                          }
                      }

                      // Try menu items
                      var menuItems = [
                          \'.menu-item[onclick*="add"]\',
                          \'.dropdown-item[onclick*="add"]\'
                      ];

                      for (var l = 0; l &lt; menuItems.length; l++) {
                          var menuItem = document.querySelector(menuItems[l]);
                          if (menuItem) {
                              menuItem.click();
                              break;
                          }
                      }
                  } else {
                      var addItem = contextMenu.querySelector(\'[onclick*="add"], :contains("Add")\');
                      if (addItem) {
                          addItem.click();
                      }
                  }
                  "success";
              }
          } else {
              "url_field_not_found";
          }
      </string>
